<!-- Speech platform: Nuance 9.x -->
<!-- This grammar allows callers to say 4  digit strings -->

<rule id='SSNLAST4' scope='public'>
 <one-of>
       <item weight=".02">
		<ruleref uri="#digits4"/>
		<tag>customer=digits4.DigitString</tag>
       </item>       

  </one-of>
</rule>


<rule id="digits4">
	<!-- collect 4 concatenated digits -->
	<item repeat="0-1"><ruleref uri="#prefixA"/></item>
	<one-of>
		<item>
			<tag>DigitString='';</tag>
			<item>
	<!--1 -->		<ruleref uri="#digit"/>
				<tag>DigitString=digit.Digit;</tag>
			</item>
			<item>
	<!--2 -->		<ruleref uri="#digit"/>
				<tag>DigitString=DigitString + digit.Digit;</tag>
			</item>
			<item>
	<!--3 -->		<ruleref uri="#digit"/>
				<tag>DigitString=DigitString + digit.Digit;</tag>
			</item>
			<item>
	<!--4-->		<ruleref uri="#digit"/>
				<tag>DigitString=DigitString + digit.Digit;</tag>
			</item>

		</item>
	</one-of>
	</rule>


